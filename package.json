{
  "name": "gdhi-app-clone",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "node build/pre_install.mjs",
    "predev": "node build/check_version.mjs",
    "dev": "vite --config build/vite.dev.config.js",
    "build": "vite build --config build/vite.prod.config.js",
    "preview": "vite preview --port 4173 --config build/vite.prod.config.js",
    "test": "vitest --environment=jsdom --config=build/vitetest.config.js",
    "test:ui": "vitest --environment=jsdom --config=build/vitetest.config.js --ui",
    "test:coverage": "vitest --environment=jsdom --config=build/vitetest.config.js --coverage",
    "test:file": "vitest run ${0} --environment=jsdom --config=build/vitetest.config.js --reporter=\"verbose\"",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "engines": {
    "node": ">= 14.0.0",
    "yarn": ">= 1.22.19"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "dateformat": "^3.0.3",
    "downloadjs": "^1.4.7",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.4",
    "pdf-lib": "^1.17.1",
    "vee-validate": "2.0.0-rc.22",
    "vue": "^2.7.7",
    "vue-body-class": "^2.0.0-beta.2",
    "vue-cookies": "^1.5.13",
    "vue-i18n": "^8.15.1",
    "vue-js-modal": "^1.3.12",
    "vue-notification": "^1.3.7",
    "vue-router": "^3.5.4",
    "vuejs-auto-complete": "^0.6.5",
    "vuejs-dialog": "^0.9.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.2",
    "@vitest/ui": "^0.28.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^1.3.0",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "chalk": "^5.2.0",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "execa": "^6.1.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^20.0.0",
    "moxios": "^0.4.0",
    "prettier": "^2.5.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.57.1",
    "semver": "^7.3.8",
    "sinon": "^4.0.0",
    "stylelint": "^14.16.1",
    "terser": "^5.14.2",
    "vite": "^3.0.2",
    "vitest": "^0.18.1",
    "vue-template-compiler": "^2.7.7"
  }
}
